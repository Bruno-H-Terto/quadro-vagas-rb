<section class="flex flex-col">
  <div>
    <h1><%= t('.dashboard') %></h1>
    <h3 class="text-lg text-green-600"><%= @imported_file.name %></h3>
  </div>

  <%= turbo_stream_from dom_id(@imported_file) %>

  <div id=<%= dom_id(@imported_file) %>>
    <%= render 'imported_files/progress', imported_file: @imported_file %>
  </div>
</section>